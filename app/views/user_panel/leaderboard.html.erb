<div class="table-responsive">
                        <table class="table table-hover table-striped">
                            <thead>
                                <tr>
                                    <th class="text-center"># Rank</th>
                                    <th class="text-center">Name</th>
                                    <th class="text-center">Country</th>
                                </tr>
                            </thead>
                            <tbody>
                            <% @data.sort.reverse!.each_with_index do |d,i|%>
                                <tr>
                                    <td><%=i+1%></td>
                                    <td>XXXX</td>
                                    <td><%= d%></td>
                                </tr>
                            <%end%>
                            </tbody>
                        </table>
                    </div>