
    <!-- social connect -->
    <div class="row text social-row">
        <div class="col-xs-offset-2 col-xs-4">
            <% if twitter_user %>
                <a class="btn btn-block btn-social btn-twitter">
                    <span class="fa fa-twitter"></span> Hello, <%= twitter_user.twitter_name %>
                </a>
            <% else %>    
                <a class="btn btn-block btn-social btn-twitter" id="sign_in" href="/auth/twitter">
                    <span class="fa fa-twitter"></span> Connect with Twitter
                </a>
            <% end %>
        </div>
        <div class="col-xs-4">
            <a class="btn btn-block btn-social btn-facebook">
                <span class="fa fa-facebook"></span> Connect with Facebook
            </a>
        </div>
    </div>
