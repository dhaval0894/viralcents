<div class="container" id="contact-container">
  <div class="col-md-12 section-heading text-center">
    <h2 class="single-animate animate-press-1">Contact Us!</h2>
  </div>  
  <div class="panel panel-default panel-contact">
    <div class="panel-body">
      <!-- Map Column -->
      <div class="col-md-8">
          <div class="google-maps">
            <!-- Embedded Google Map -->
            <iframe src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d3680.9432631171703!2d75.86565695011569!3d22.693155934181497!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x0%3A0x0!2zMjLCsDQxJzM1LjMiTiA3NcKwNTInMDQuMyJF!5e0!3m2!1sen!2sin!4v1460809162917" width="600" height="450" frameborder="0" style="border:0" allowfullscreen></iframe>
          </div>
      </div>
      <!-- Contact Details Column -->
      <div class="col-md-4">
          <h3 class="lead">If you have any query or suggestions, drop us a line.</h3>
          
          <form action="">
              <div class="form-group">
                <input type="text" class="form-control" placeholder="Enter Email">
              </div>
              <div class="form-group">
                 <textarea class="form-control" rows="5" id="message" placeholder="Enter Message"></textarea>
              </div>
              <div class="col-md-4 col-md-offset-4 single-animate animate-features-3">
                <a href="#" class="btn btn-primary btn-block">Submit</a>
              </div>
          </form>
      </div>
    </div>
  </div>
</div>